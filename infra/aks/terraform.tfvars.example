# AKS terraform.tfvars example
# Copy to terraform.tfvars and update values

resource_group_name    = "rally-mlops-rg"
location              = "eastus"
cluster_name          = "rally-mlops-aks"
kubernetes_version    = "1.28"
system_pool_vm_size   = "Standard_B2s"
worker_pool_vm_size   = "Standard_D2s_v3"
worker_pool_node_count = 3
storage_account_name  = "rallystoragexyz"  # Must be globally unique
environment           = "dev"
